# Core service ports
POSTGRES_PORT=5432
REDIS_PORT=6379
QDRANT_PORT=6333
MINIO_API_PORT=9000
MINIO_CONSOLE_PORT=9001
BACKEND_PORT=8000
FRONTEND_PORT=3000

# Postgres
POSTGRES_DB=paperta
POSTGRES_USER=paperta
POSTGRES_PASSWORD=paperta_dev_password

# Redis
REDIS_URL=redis://redis:6379/0

# Qdrant
QDRANT_URL=http://qdrant:6333

# MinIO
MINIO_ROOT_USER=paperta
MINIO_ROOT_PASSWORD=paperta_dev_password
MINIO_ENDPOINT=http://minio:9000
MINIO_BUCKET=paperta-artifacts

# Backend app
ENV=development
API_BASE_URL=http://localhost:8000
FRONTEND_BASE_URL=http://localhost:3000

# Provider keys (set only what you use)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=
